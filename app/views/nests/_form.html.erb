<%= simple_form_for(@user,@nest) do |a| %>
  <%= a.input :address %>
  <%= a.input :description %>
  <%= a.input :opening_hour %>
  <%= a.input :closing_hour %>
  <%= a.input :max_capacity %>
  <%= a.input :price %>
  <%= a.input :city %>
  <%= a.input :zipcode %>
  <%= a.input :wifi %>
  <%= a.input :printer %>
  <%= a.input :coffee %>
  <%= a.input :tea %>
  <%= a.input :microwave %>
  <%= a.input :other_equipment %>
  <%= a.input :phoning_room %>

  <%= a.submit class: "btn btn-primary"%>
<% end %>
